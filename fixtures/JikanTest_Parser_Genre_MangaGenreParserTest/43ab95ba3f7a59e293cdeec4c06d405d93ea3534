        <h2 class="h2_manga_title"><a href="https://myanimelist.net/manga/8848/Sun-Ken_Rock" class="link-title">Sun-Ken Rock</a></h2></div>
    </div>

    <div class="prodsrc">
      <div class="video"></div>
      <div class="info">
        <span class="item">Manga, 2006</span>
        <span class="item finished">Finished</span>
        <span class="item"><span class="volume js-volume">25</span> vol, <span class="chapter js-chapter">181</span> chp</span>
      </div>
      <div class="broadcast">
              </div>
    </div>

    <div class="genres js-genre" id="8848">
      <div class="genres-inner js-genre-inner"><span class="genre">
            <a href="/manga/genre/1/Action" title="Action">Action</a>
          </span><span class="genre">
            <a href="/manga/genre/4/Comedy" title="Comedy">Comedy</a>
          </span><span class="genre">
            <a href="/manga/genre/8/Drama" title="Drama">Drama</a>
          </span><span class="genre explicit">
            <a href="/manga/genre/9/Ecchi" title="Ecchi">Ecchi</a>
          </span></div>
    </div>
