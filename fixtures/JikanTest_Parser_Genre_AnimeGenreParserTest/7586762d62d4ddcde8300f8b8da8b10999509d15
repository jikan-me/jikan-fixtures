<div class="js-anime-category-producer seasonal-anime js-seasonal-anime js-anime-type-all js-anime-type-1" data-genre="1,4,37,31">
  <div>
    <div class="title"><div class="title-text">
        <h2 class="h2_anime_title"><a href="https://myanimelist.net/anime/32182/Mob_Psycho_100" class="link-title">Mob Psycho 100</a></h2></div>
      <span style="display: none;" class="js-members">1728992</span>
      <span style="display: none;" class="js-score">8.48</span>
      <span style="display: none;" class="js-start_date">20160711</span>
      <span style="display: none;" class="js-title">Mob Psycho 100</span>
    </div>

    <div class="prodsrc">
      <div class="video"><a href="https://myanimelist.net/anime/32182/Mob_Psycho_100/video" class="ga-click" title="Watch Episode Video"><i class="malicon malicon-movie-episode"></i></a></div>
