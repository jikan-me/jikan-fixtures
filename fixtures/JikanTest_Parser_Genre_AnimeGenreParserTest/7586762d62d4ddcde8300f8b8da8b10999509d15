      <div class="broadcast"><a href="javascript:void(0)" onclick="return false;" class="js-broadcast-button ga-click" data-title="Mob Psycho 100" data-subtitle="" data-raw='{"data":[{"platform":{"id":1,"name":"Crunchyroll","icon":"crunchyroll","type":1},"available":true,"url":"http:\/\/www.crunchyroll.com\/series-270683"},{"platform":{"id":4,"name":"Funimation","icon":"funimation","type":1},"available":false,"url":"https:\/\/www.funimation.com\/shows\/mob-psycho-100"},{"platform":{"id":2,"name":"Netflix","icon":"netflix","type":1},"available":false,"url":"https:\/\/www.netflix.com\/"}],"count":{"available":1,"typicals":3,"others":0,"total":3}}' data-ga-click-type="broadcast-tile-streaming-icon" data-ga-click-param="aid:32182"><i class="malicon malicon-streaming"></i></a></div>
